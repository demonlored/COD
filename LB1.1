#include <stdio.h>

int main()
{
     int a=0,b=0,c=0,m=0,mx=0;
    printf("Input a,b,c : ");
    int check = scanf("%d %d %d",&a,&b,&c);
    if(check == 3)
    {
        if ((a>b)&&(b>c))
        {
            mx = a-3;
            m = c+2;
        }
        if ((a<b)&&(b<c))
        {
            m = a+2;
            mx = c-3;
        }
        if ((a<c)&&(c<b))
        {
            mx = b-3;
            m = a+2;
        }
        if ((b<c)&&(c<a))
        {
            mx = a-3;
            m = b+2;
        }
       printf("%d %d \n", m, mx);
    }
    else
    {
        printf("wrong data\n");
    }
    return 0;
}
