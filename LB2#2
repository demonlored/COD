#include <stdio.h>
#include <stdlib.h>
#include <math.h>
 
int main()
{
    int a,b,h,x;
    double y;
    printf("input a\n");
    scanf("%d", &a);
    printf("input b\n");
    scanf("%d", &b);
    printf("input h\n");
    scanf("%d", &h);
    for(a=a;a<=b;a+=h)
    {
        x=a;
        y=log(1+x);
        printf("h = %d\ty = %lf\n", h, y);
    }
 
    return 0;
}
